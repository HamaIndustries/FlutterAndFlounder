	"authors": [
		"Superkat32"
	],
	"contact": {
		"sources": "https://github.com/Superkat32/FlutterAndFlounder"
	},
	"license": "MIT",
	"icon": "assets/flutterandflounder/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"net.superkat.flutterandflounder.FlutterAndFlounderMain"
		],
		"client": [
			"net.superkat.flutterandflounder.FlutterAndFlounderClient"
		]
	},
	"mixins": [
		"flutterandflounder.mixins.json"
	],
	"accessWidener": "flutterandflounder.accesswidener",
	"depends": {
		"fabricloader": ">=0.15.2",
		"minecraft": "~1.20.4",
		"java": ">=17",
